/*css reset*/
* {
  pading: 0;
  margin: 0;
}
body {
  background: #f6f6f6;
  font: 12px/1.5 tahoma, airal, Microsoft YaHei, sans-serif;
}
li {
  list-style: none;
}
img {
  display: block;
  border: none;
}
input[type='checkbox'] {
  cursor: pointer;
}

/* 定宽布局 */
.w {
  width: 1080px;
  margin: 0 auto;
  position: relative;
  overflow: hidden;
}

header .container {
  height: 552px;
  width: 100px;
  background-color: #ccc;
}

